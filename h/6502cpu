#include "6502cmap.h"

#define NFLAG 0x80
#define VFLAG 0x40
#define UFLAG 0x20
#define BFLAG 0x10
#define DFLAG 0x08
#define IFLAG 0x04
#define ZFLAG 0x02
#define CFLAG 0x01
#define NOTNFLAG 0x7F
#define NOTVFLAG 0xBF
#define NOTUFLAG 0xDF
#define NOTBFLAG 0xEF
#define NOTDFLAG 0xF7
#define NOTIFLAG 0xFB
#define NOTZFLAG 0xFD
#define NOTCFLAG 0xFE

#define ADDRESSING_MODIFYFLAG 0x01
#define ADDRESSING_BASEFLAG 0x05

#define R6502_REGULAR_TRANSITION 0
#define R6502_YIELD_TRANSITION 1
#define R6502_RESET_TRANSITION 2

#define R6502_YIELD_SEQUENCE 256
#define R6502_INTERRUPT_SEQUENCE 257
#define R6502_RESET_SEQUENCE 258
#define R6502_READ_STRETCH_SEQUENCE 259
#define R6502_DISCARD_STRETCH_SEQUENCE 260
#define R6502_WRITE_STRETCH_SEQUENCE 261
#define R6502_MAIN_SEQUENCE_COUNT 262

#define R6502_OPCODE_07_SEQUENCE 262
#define R6502_SEQUENCE_COUNT 263

#define R6502_EXTRA_SEQUENCE_COUNT (R6502_SEQUENCE_COUNT - R6502_MAIN_SEQUENCE_COUNT)

#define R6502_LOG2_MAX_SEQUENCE_LENGTH 3

/*with optimisations applied, assembly language addressing routines are not directly callable from C*/
#ifdef BEEBIT_NO_ASM
#define R6502_INVOKE_ADDRESSING(MAP, ADDRCODE, CALLBACKS) \
  MAP->addressingops[(((ADDRCODE)>>28)|((ADDRCODE)<<4))&0x1F].op(MAP, (ADDRCODE)|ADDRESSING_MODIFYFLAG, CALLBACKS);
#else
#define R6502_INVOKE_ADDRESSING(MAP, ADDRCODE, CALLBACKS) \
  r6502invokeaddressing(MAP, ADDRCODE, CALLBACKS);
#endif

/*an assembly language version of R6502_ADDRESS_FOR_READ appears in h/6502cpus*/
#define R6502_ADDRESS_FOR_READ(MAP, ADDRCODE, RDS, RD1S, RD2S, RD) \
  { \
    const R6502ADDRESSINGCALLBACKS rdcallbacks = {RDS, RD1S, RD2S, RD}; \
    R6502_INVOKE_ADDRESSING(MAP, (ADDRCODE)&~ADDRESSING_MODIFYFLAG, &rdcallbacks) \
  }

/*an assembly language version of R6502_ADDRESS_FOR_MODIFY appears in h/6502cpus*/
#define R6502_ADDRESS_FOR_MODIFY(MAP, ADDRCODE, MDS, MD1S, MD2S, MD) \
  { \
    const R6502ADDRESSINGCALLBACKS mdcallbacks = {MDS, MD1S, MD2S, MD}; \
    R6502_INVOKE_ADDRESSING(MAP, ADDRCODE, &mdcallbacks) \
  }

#define R6502_CYCLE_STRETCH_RETURN(MAP) \
  return (*MAP->cycleaction)(MAP);

#define R6502_CYCLE_RETURN(MAP) \
  { \
    uintptr_t interruptqueue = cmap->interruptqueue; \
    interruptqueue = (interruptqueue<<4) | (interruptqueue&0x0F); \
    if (!(interruptqueue & R6502_NMI)) \
      interruptqueue |= R6502_NMI_ENABLE; \
    cmap->interruptqueue = interruptqueue; \
    return (*MAP->cycleaction)(MAP); \
  }

#define R6502_CYCLE_ZPG_READ_RETURN(MAP, ZCODE) \
  { \
    MAP->m = MAP->pagezero[(ZCODE)>>24]; \
    R6502_CYCLE_RETURN(MAP) \
  }

#define R6502_CYCLE_ZPG_WRITE_RETURN(MAP, ZCODE) \
  { \
    MAP->pagezero[(ZCODE)>>24] = MAP->m; \
    R6502_CYCLE_RETURN(MAP) \
  }

#define R6502_CYCLE_STACK_READ_RETURN(MAP, SPCODE) \
  { \
    MAP->m = MAP->pageone[(SPCODE)>>24]; \
    R6502_CYCLE_RETURN(MAP) \
  }

#define R6502_CYCLE_STACK_WRITE_RETURN(MAP, SPCODE) \
  { \
    MAP->pageone[(SPCODE)>>24] = MAP->m; \
    R6502_CYCLE_RETURN(MAP) \
  }

#define R6502_CYCLE_VECTOR_READ_RETURN(MAP, ADDRCODE) \
  { \
    MAP->m = MAP->pageminusone[((ADDRCODE)>>16)&0xFF]; \
    R6502_CYCLE_RETURN(MAP) \
  }

#define R6502_CYCLE_READ_RETURN(MAP, ADDRCODE) \
  { \
    R6502_ADDRESS_FOR_READ(MAP, ADDRCODE, r6502cyclereadspecialreturn, r6502cyclestretch1readspecialreturn, r6502cyclestretch2readspecialreturn, r6502cyclereadmappedreturn) \
    return MAP; \
  }

#define R6502_CYCLE_DISCARD_RETURN(MAP, ADDRCODE) \
  { \
    R6502_ADDRESS_FOR_READ(MAP, ADDRCODE, r6502cyclediscardspecialreturn, r6502cyclestretch1discardspecialreturn, r6502cyclestretch2discardspecialreturn, r6502cyclediscardmappedreturn) \
    return MAP; \
  }

#define R6502_CYCLE_WRITE_RETURN(MAP, ADDRCODE) \
  { \
    R6502_ADDRESS_FOR_MODIFY(MAP, ADDRCODE, r6502cyclewritespecialreturn, r6502cyclestretch1writespecialreturn, r6502cyclestretch2writespecialreturn, r6502cyclewritemappedreturn) \
    return MAP; \
  }

#define R6502_CYCLE_FETCH_RETURN(MAP, PCCODE) \
  { \
    MAP->sync_pc_code = PCCODE; \
    R6502_ADDRESS_FOR_READ(MAP, MAP->sync_pc_code, r6502cyclefetchspecialreturn, r6502cyclestretch1fetchspecialreturn, r6502cyclestretch2fetchspecialreturn, r6502cyclefetchmappedreturn) \
    return MAP; \
  }

#define R6502_CYCLE_EARLY_INTERRUPT_FETCH_RETURN(MAP, PCCODE) \
  { \
    MAP->sync_pc_code = PCCODE; \
    R6502_ADDRESS_FOR_READ(MAP, MAP->sync_pc_code, r6502cycleearlyinterruptfetchspecialreturn, r6502cycleearlyinterruptstretch1fetchspecialreturn, r6502cycleearlyinterruptstretch2fetchspecialreturn, r6502cycleearlyinterruptfetchmappedreturn) \
    return MAP; \
  }

#define R6502_CYCLE_INTERRUPT_CHECK_FETCH_RETURN(MAP, PCCODE) \
  { \
    MAP->sync_pc_code = PCCODE; \
    R6502_ADDRESS_FOR_READ(MAP, MAP->sync_pc_code, r6502cycleinterruptfetchspecialreturn, r6502cycleinterruptstretch1fetchspecialreturn, r6502cycleinterruptstretch2fetchspecialreturn, r6502cycleinterruptfetchmappedreturn) \
    return MAP; \
  }

#define R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(MAP) \
  R6502_CYCLE_INTERRUPT_CHECK_FETCH_RETURN(MAP, ((MAP)->sync_pc_code + 0x10000) & 0xFFFFFFFF);

#define R6502_CYCLE_ADDRESSED_FETCH_RETURN(MAP) \
  R6502_CYCLE_FETCH_RETURN(MAP, ((MAP)->last_pc_code + 0x10000) & 0xFFFFFFFF);

#define R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(MAP) \
  R6502_CYCLE_INTERRUPT_CHECK_FETCH_RETURN(MAP, ((MAP)->last_pc_code + 0x10000) & 0xFFFFFFFF);

#define R6502_CHECK_FOR_BREAK(MAP) \
  { \
    if ((MAP)->lbreak) \
      (MAP)->transition1 = R6502_RESET_TRANSITION; \
  }


#define R6502_IMPLIED_SEQUENCE(IMPLIED_STEP) \
    {r6502operandstep, IMPLIED_STEP, NULL}

#define R6502_PUSH_SEQUENCE(TO_ADDRESS_STEP) \
    {r6502operandstep, TO_ADDRESS_STEP, r6502pushedstep, NULL}

