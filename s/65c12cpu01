|s2_r65c12linearmodifyabsx_u1p2l|
  R6502_LOAD_ADDRESSING_LINK r5
  R6502_OPCODE_CYCLES 1,1
  B sc_r65c12linearmodifyabsx_p2
|s1_r65c12linearmodifyabsx_u1p2l|
  R6502_LOAD_ADDRESSING_LINK r5
  R6502_OPCODE_CYCLES 1,2
sc_r65c12linearmodifyabsx_p2
  R6502_OPCODE_WRITE_SPECIAL
  LDR r2,[r0,#:INDEX:R6502_X_SHIFT]
  LDRB r3,[r0,#:INDEX:R6502_M]
  ADDS r4,r2,r4,LSL #24
  ORR r4,r3,r4
  ORR r4,r4,#ADDRESSING_BASEFLAG:SHL:8
  BCC s_r65c12linearmodifyabsx_p2cc
  R6502_OPCODE_CYCLES 1,1
  R6502_OPCODE_WRITE_SPECIAL
  MOV r6,r4,ROR #8
  ADD r6,r6,#1:SHL:24
  R6502_MODIFY_BRANCH r65c12linearmodifyabs_u1p3
  LDRB r4,[r1]
  MOV r6,r1
  R6502_OPCODE_CYCLES 3,0
  R6502_LINEAR_MODIFY_MAPPED_ABS_OPCODE_FINISH
|s_r65c12linearmodifyabsx_u1p2l|
  R6502_LOAD_ADDRESSING_LINK r5
  R6502_OPCODE_CYCLES 1,0
s_r65c12linearmodifyabsx_p2
  R6502_OPCODE_WRITE_SPECIAL
  LDR r2,[r0,#:INDEX:R6502_X_SHIFT]
  LDRB r3,[r0,#:INDEX:R6502_M]
  ADDS r4,r2,r4,LSL #24
  ORR r4,r3,r4
  ORR r4,r4,#ADDRESSING_BASEFLAG:SHL:8
  BCC s_r65c12linearmodifyabsx_p2cc
  R6502_OPCODE_CYCLES 1,0
  R6502_OPCODE_WRITE_SPECIAL
  MOV r6,r4,ROR #8
  ADD r6,r6,#1:SHL:24
  R6502_MODIFY_BRANCH r65c12linearmodifyabs_u1p3
  LDRB r4,[r1]
  MOV r6,r1
  R6502_OPCODE_CYCLES 3,0
  R6502_LINEAR_MODIFY_MAPPED_ABS_OPCODE_FINISH
s_r65c12linearmodifyabsx_p2cc
  MOV r6,r4,ROR #8
  R6502_MODIFY_BRANCH r65c12linearmodifyabsx_u1p3
  LDRB r4,[r1]
  MOV r6,r1
  R6502_OPCODE_CYCLES 4,0
  R6502_LINEAR_MODIFY_MAPPED_ABS_OPCODE_FINISH

|s2_r65c12modifyabsx_u1p2l|
  R6502_LOAD_ADDRESSING_LINK r5
  R6502_OPCODE_CYCLES 1,1
  B s_r65c12modifyabsx_p2
|s1_r65c12modifyabsx_u1p2l|
  R6502_LOAD_ADDRESSING_LINK r5
  R6502_OPCODE_CYCLES 1,2
  B s_r65c12modifyabsx_p2
|s_r65c12modifyabsx_u1p2l|
  R6502_LOAD_ADDRESSING_LINK r5
  R6502_OPCODE_CYCLES 1,0
s_r65c12modifyabsx_p2
  R6502_OPCODE_WRITE_SPECIAL
  R6502_OPCODE_CYCLES 1,0
  LDRB r3,[r0,#:INDEX:R6502_M]
  ADDS r4,r2,r4,LSL #24
  ORR r4,r3,r4
  ORR r4,r4,#ADDRESSING_BASEFLAG:SHL:8
  BCC s_r65c12modifyabsx_p2cc
  R6502_OPCODE_WRITE_SPECIAL
  MOV r6,r4,ROR #8
  ADD r6,r6,#1:SHL:24
  R6502_MODIFY_BRANCH r65c12linearmodifyabs_u1p3
  LDRB r4,[r1]
  MOV r6,r1
  R6502_OPCODE_CYCLES 3,0
  R6502_LINEAR_MODIFY_MAPPED_ABS_OPCODE_FINISH
s_r65c12modifyabsx_p2cc
  MOV r6,r4,ROR #8
  R6502_MODIFY_BRANCH r65c12modifyabs_u1p3
  LDRB r4,[r1]
  MOV r6,r1
  R6502_OPCODE_CYCLES 3,0
  R65C12_MODIFY_MAPPED_ABS_OPCODE_FINISH

