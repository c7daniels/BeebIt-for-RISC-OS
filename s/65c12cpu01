  MACRO
  BRK_ACTION
  ADD r7,r7,#2:SHL:16
  LDR r4,[r0,#:INDEX:R6502_SP_CODE]
  MOV r2,r7,LSR #24
  LDR r5,[r0,#:INDEX:R6502_PAGEONE]
  SUB r3,r4,#1:SHL:24
  STRB r2,[r5,r4,LSR #24]
  MOV r2,r7,LSR #16
  SUB r4,r3,#1:SHL:24
  STRB r2,[r5,r3,LSR #24]
  LDR r3,[r0,#:INDEX:R6502_INTERRUPTQUEUE]
  TST r3,#(R6502_IRQ_ENABLE<<12)
  BICNE r2,r8,#IFLAG
  MOVEQ r2,r8
  BIC r8,r8,#DFLAG
  STRB r2,[r5,r4,LSR #24]
  SUB r4,r4,#1:SHL:24
  STR r4,[r0,#:INDEX:R6502_SP_CODE]
  BIC r3,r3,#(R6502_IRQ_ENABLE<<4)|R6502_IRQ_ENABLE
  LDR r5,[r0,#:INDEX:R6502_PAGEMINUSONE]
  BIC r3,r3,#(R6502_IRQ_ENABLE<<8)
  LDR r1,[r5,#&FC]
  MOV r7,#ADDRESSING_BASEFLAG
  MOV r1,r1,LSR #16
  STR r3,[r0,#:INDEX:R6502_INTERRUPTQUEUE]
  ORR r6,r7,r1,LSL #16
  ORR r7,r7,r1,LSL #16
  MEND

  MACRO
  INTERRUPT_ACTION
  LDR r4,[r0,#:INDEX:R6502_SP_CODE]
  MOV r2,r7,LSR #24
  LDR r5,[r0,#:INDEX:R6502_PAGEONE]
  SUB r3,r4,#1:SHL:24
  STRB r2,[r5,r4,LSR #24]
  MOV r2,r7,LSR #16
  SUB r4,r3,#1:SHL:24
  STRB r2,[r5,r3,LSR #24]
  LDR r3,[r0,#:INDEX:R6502_INTERRUPTQUEUE]
  TST r3,#(R6502_IRQ_ENABLE<<12)
  BICNE r2,r8,#BFLAG|IFLAG
  BICEQ r2,r8,#BFLAG
  BIC r8,r8,#DFLAG
  TST r3,#(R6502_NMI_ENABLE<<12)
  STRB r2,[r5,r4,LSR #24]
  SUB r4,r4,#1:SHL:24
  TSTNE r3,#(R6502_NMI<<12)
  STR r4,[r0,#:INDEX:R6502_SP_CODE]
  BICNE r3,r3,#(R6502_NMI_IRQ_ENABLE<<4)|R6502_NMI_IRQ_ENABLE
  BICEQ r3,r3,#(R6502_IRQ_ENABLE<<4)|R6502_IRQ_ENABLE
  LDR r5,[r0,#:INDEX:R6502_PAGEMINUSONE]
  BICNE r3,r3,#(R6502_NMI_IRQ_ENABLE<<8)
  BICEQ r3,r3,#(R6502_IRQ_ENABLE<<8)
  ANDNE r2,r3,#(R6502_NMI<<8)|(R6502_NMI<<4)
  LDRNE r1,[r5,#&F8]
  LDREQ r1,[r5,#&FC]
  EORNE r2,r2,#(R6502_NMI<<8)|(R6502_NMI<<4)
  MOV r7,#ADDRESSING_BASEFLAG
  ORRNE r3,r3,r2,LSR #6
  MOV r1,r1,LSR #16
  STR r3,[r0,#:INDEX:R6502_INTERRUPTQUEUE]
  ORR r6,r7,r1,LSL #16
  ORR r7,r7,r1,LSL #16
  MEND

  MACRO
  TSB_ACTION
  LDRB r1,[r0,#:INDEX:R6502_A]
  TST r1,r4
  ORR r4,r4,r1
  BICNE r8,r8,#ZFLAG
  ORREQ r8,r8,#ZFLAG
  MEND

  MACRO
  ASL_ACTION
  BIC r8,r8,#NFLAG|ZFLAG|CFLAG
  MOVS r3,r4,LSL #25
  ORRMI r8,r8,#NFLAG
  MOV r4,r3,LSR #24
  ORREQ r8,r8,#ZFLAG
  ORRCS r8,r8,#CFLAG
  MEND

  MACRO
  TRB_ACTION
  LDRB r1,[r0,#:INDEX:R6502_A]
  TST r1,r4
  BIC r4,r4,r1
  BICNE r8,r8,#ZFLAG
  ORREQ r8,r8,#ZFLAG
  MEND

  MACRO
  INCA_ACTION
  LDR r2,[r0,#:INDEX:R6502_A_SHIFT]
  BIC r8,r8,#NFLAG|ZFLAG
  ADDS r2,r2,#1:SHL:24
  ORRMI r8,r8,#NFLAG
  STR r2,[r0,#:INDEX:R6502_A_SHIFT]
  ORREQ r8,r8,#ZFLAG
  MEND

|r65c12brkroutine|
  R6502_OPCODE_PUSH
  R6502_OPERAND_READ_BRANCH r65c12brk_p1
r65c12brk_rs1
  R6502_OPCODE_CYCLES 6,0
  BRK_ACTION
  R6502_OPCODE_INTERRUPT_FETCH 0
s2_r65c12brk_p1
  R6502_OPCODE_CYCLES 0,2
  B s_r65c12brk_p1
s1_r65c12brk_p1
  R6502_OPCODE_CYCLES 0,1
s_r65c12brk_p1
  R6502_OPCODE_READ_SPECIAL
  B r65c12brk_rs1

|r65c12interruptroutine|
  R6502_OPCODE_PUSH
  R6502_NO_OPERAND_READ_BRANCH r65c12interrupt_p1
r65c12interrupt_rs1
  R6502_OPCODE_CYCLES 6,0
  INTERRUPT_ACTION
  R6502_OPCODE_INTERRUPT_FETCH 0
s2_r65c12interrupt_p1
  R6502_OPCODE_CYCLES 0,2
  B s_r65c12interrupt_p1
s1_r65c12interrupt_p1
  R6502_OPCODE_CYCLES 0,1
s_r65c12interrupt_p1
  R6502_OPCODE_READ_SPECIAL
  B r65c12interrupt_rs1

|r65c12tsbzpgroutine|
  R6502_OPCODE_PUSH
  R6502_MODIFY_ZPG_OPCODE_START
  TSB_ACTION
  R6502_MODIFY_ZPG_OPCODE_FINISH

|r65c12tsbabsroutine|
  R6502_OPCODE_PUSH
  R65C12_LINEAR_MODIFY_ABS_OPCODE_START r65c12tsb
  TSB_ACTION
  R6502_LINEAR_MODIFY_ABS_OPCODE_FINISH

|r65c12aslabsroutine|
  R6502_OPCODE_PUSH
  R65C12_MODIFY_ABS_OPCODE_START r6502asl
  ASL_ACTION
  R65C12_MODIFY_ABS_OPCODE_FINISH

|r65c12bplroutine|
  R6502_OPCODE_PUSH
  R65C12_BRANCH_IF_CLEAR_OPCODE_RETURN NFLAG

|r65c12trbzpgroutine|
  R6502_OPCODE_PUSH
  R6502_MODIFY_ZPG_OPCODE_START
  TRB_ACTION
  R6502_MODIFY_ZPG_OPCODE_FINISH

|r65c12incaroutine|
  R6502_OPCODE_PUSH
  INCA_ACTION
  R6502_IMPLIED_OPCODE_RETURN

|r65c12trbabsroutine|
  R6502_OPCODE_PUSH
  R65C12_LINEAR_MODIFY_ABS_OPCODE_START r65c12trb
  TRB_ACTION
  R6502_LINEAR_MODIFY_ABS_OPCODE_FINISH

|r65c12aslabsxroutine|
  R6502_OPCODE_PUSH
  R65C12_MODIFY_ABSX_OPCODE_START r6502asl
  ASL_ACTION
  R65C12_MODIFY_ABS_OPCODE_FINISH

r65c12tsb
  TSB_ACTION
  MOV pc,lr

r65c12trb
  TRB_ACTION
  MOV pc,lr

|s2_r65c12modifyabs_u1p2l|
  R6502_LOAD_ADDRESSING_LINK r5
  R6502_OPCODE_CYCLES 1,1
  B s_r65c12modifyabs_p2
|s1_r65c12modifyabs_u1p2l|
  R6502_LOAD_ADDRESSING_LINK r5
  R6502_OPCODE_CYCLES 1,2
  B s_r65c12modifyabs_p2
|s_r65c12modifyabs_u1p2l|
  R6502_LOAD_ADDRESSING_LINK r5
  R6502_OPCODE_CYCLES 1,0
s_r65c12modifyabs_p2
  R6502_OPCODE_WRITE_SPECIAL
  LDRB r3,[r0,#:INDEX:R6502_M]
  MOV r6,#ADDRESSING_BASEFLAG
  ORR r6,r6,r4,LSL #16
  ORR r6,r6,r3,LSL #24
  R6502_MODIFY_BRANCH r65c12modifyabs_u1p3
  LDRB r4,[r1]
  MOV r6,r1
  R6502_OPCODE_CYCLES 3,0
  R65C12_MODIFY_MAPPED_ABS_OPCODE_FINISH
|s2_r65c12modifyabs_u2p3l|
  R6502_LOAD_ADDRESSING_LINK r5
  R6502_OPCODE_CYCLES 2,2
  B sc_r65c12modifyabs_p3
|s1_r65c12modifyabs_u2p3l|
  R6502_LOAD_ADDRESSING_LINK r5
  R6502_OPCODE_CYCLES 2,1
  B sc_r65c12modifyabs_p3
|s_r65c12modifyabs_u2p3l|
  R6502_LOAD_ADDRESSING_LINK r5
  R6502_OPCODE_CYCLES 2,0
  B s_r65c12modifyabs_p3
s2_r65c12modifyabs_u1p3
  R6502_OPCODE_CYCLES 1,1
  B sc_r65c12modifyabs_p3
s1_r65c12modifyabs_u1p3
  R6502_OPCODE_CYCLES 1,2
sc_r65c12modifyabs_p3
  R6502_OPCODE_WRITE_SPECIAL
  LDRB r4,[r0,#:INDEX:R6502_M]
  R6502_OPCODE_CYCLES 1,1
  R6502_OPCODE_WRITE_SPECIAL
  R6502_OPCODE_CYCLES 1,1
  R65C12_MODIFY_SPECIAL_ABS_OPCODE_FINISH
s_r65c12modifyabs_u1p3
  R6502_OPCODE_CYCLES 1,0
s_r65c12modifyabs_p3
  R6502_OPCODE_WRITE_SPECIAL
  LDRB r4,[r0,#:INDEX:R6502_M]
  R6502_OPCODE_CYCLES 1,0
  R6502_OPCODE_WRITE_SPECIAL
  R6502_OPCODE_CYCLES 1,0
  R65C12_MODIFY_SPECIAL_ABS_OPCODE_FINISH

