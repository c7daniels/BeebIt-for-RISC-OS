#ifdef BEEBIT_NO_ASM
// IMPLIED STEPS
R6502CPUMAP* r65c12fastnopstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t pc_code = (cmap->sync_pc_code + 0x10000) & 0xFFFFFFFF;
  R6502_CYCLE_FETCH_RETURN(cmap, pc_code)
}

R6502CPUMAP* r65c12pulledxstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->x = cmap->m;
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->x & NFLAG) | (cmap->x == 0 ? ZFLAG : 0);
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r65c12pulledystep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->y = cmap->m;
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->y & NFLAG) | (cmap->y == 0 ? ZFLAG : 0);
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

// LOAD STEPS
R6502CPUMAP* r65c12sumtoimmediatestep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t last_pc_code;
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  last_pc_code = (cmap->sync_pc_code + 0x10000) & 0xFFFFFFFF;
  cmap->last_pc_code = last_pc_code;
  cmap->decimalflags_address_code = last_pc_code;
  R6502_CYCLE_READ_RETURN(cmap, last_pc_code)
}

R6502CPUMAP* r65c12tozpgstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t last_pc_code;
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  last_pc_code = (cmap->sync_pc_code + 0x10000) & 0xFFFFFFFF;
  cmap->last_pc_code = last_pc_code;
  cmap->address_code = (((uintptr_t)cmap->m)<<24);
  R6502_CYCLE_READ_RETURN(cmap, last_pc_code)
}

R6502CPUMAP* r65c12sumloadzpgstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t z_code;
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  cmap->last_pc_code = (cmap->sync_pc_code + 0x10000) & 0xFFFFFFFF;
  z_code = (((uintptr_t)cmap->m)<<24);
  cmap->decimalflags_address_code = (z_code>>8) | ADDRESSING_BASEFLAG;
  R6502_CYCLE_ZPG_READ_RETURN(cmap, z_code)
}

R6502CPUMAP* r65c12sumlatchabslostep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  cmap->last_pc_code = (cmap->sync_pc_code + 0x20000) & 0xFFFFFFFF;
  cmap->latch = (((uintptr_t)cmap->m)<<16);
  cmap->decimalflags_address_code = cmap->last_pc_code;
  R6502_CYCLE_READ_RETURN(cmap, cmap->last_pc_code)
}

R6502CPUMAP* r65c12breadcrumbloadabstomodifystep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  cmap->address_code = (((uintptr_t)cmap->m)<<24) | cmap->latch | ADDRESSING_BASEFLAG;
  cmap->modifyoutofpage = 0;
  R6502_CYCLE_READ_RETURN(cmap, cmap->address_code)
}

R6502CPUMAP* r65c12sumlatchzpgindlostep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t z_code;
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  z_code = (cmap->address_code+0x1000000) & 0xFF000000;
  cmap->latch = (((uintptr_t)cmap->m)<<16);
  cmap->decimalflags_address_code = (z_code>>8) | ADDRESSING_BASEFLAG;
  R6502_CYCLE_ZPG_READ_RETURN(cmap, z_code)
}

R6502CPUMAP* r65c12sumloadzpgxstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t z_code;
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  z_code = (cmap->address_code+(((uintptr_t)cmap->x)<<24)) & 0xFF000000;
  cmap->decimalflags_address_code = (z_code>>8) | ADDRESSING_BASEFLAG;
  R6502_CYCLE_ZPG_READ_RETURN(cmap, z_code)
}

R6502CPUMAP* r65c12sumloadzpgindstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t z_code;
  uintptr_t address_code;
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  z_code = cmap->address_code;
  address_code = (((uintptr_t)cmap->m)<<24) | cmap->latch | ADDRESSING_BASEFLAG;
  cmap->decimalflags_address_code = (z_code>>8) | ADDRESSING_BASEFLAG;
  R6502_CYCLE_READ_RETURN(cmap, address_code)
}

R6502CPUMAP* r65c12breadcrumbloadfullabsxstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  uintptr_t sum = cmap->latch + (((uintptr_t)cmap->x)<<16);
  cmap->address_code = ((((uintptr_t)cmap->m)<<24) + sum) | ADDRESSING_BASEFLAG;
  R6502_CYCLE_READ_RETURN(cmap, cmap->address_code)
}

R6502CPUMAP* r65c12sumloadabsxstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t sum = cmap->latch + (((uintptr_t)cmap->x)<<16);
  uintptr_t updated_address_code = ((((uintptr_t)cmap->m)<<24) + sum) | ADDRESSING_BASEFLAG;
  uintptr_t address_code;
  if (sum & 0x01000000)
  {
    cmap->cpu_state = (R6502OP*)incremented_cpu_state;
    address_code = cmap->decimalflags_address_code;
  }
  else
  {
    cmap->cpu_state = ((R6502OP*)incremented_cpu_state)+1;
    address_code = updated_address_code;
  }
  cmap->address_code = updated_address_code;
  R6502_CYCLE_READ_RETURN(cmap, address_code)
}

R6502CPUMAP* r65c12sumloadabsystep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t sum = cmap->latch + (((uintptr_t)cmap->y)<<16);
  uintptr_t updated_address_code = ((((uintptr_t)cmap->m)<<24) + sum) | ADDRESSING_BASEFLAG;
  uintptr_t address_code;
  if (sum & 0x01000000)
  {
    cmap->cpu_state = (R6502OP*)incremented_cpu_state;
    address_code = cmap->decimalflags_address_code;
  }
  else
  {
    cmap->cpu_state = ((R6502OP*)incremented_cpu_state)+1;
    address_code = updated_address_code;
  }
  cmap->address_code = updated_address_code;
  R6502_CYCLE_READ_RETURN(cmap, address_code)
}

R6502CPUMAP* r65c12loadabsxtomodifystep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t sum = cmap->latch + (((uintptr_t)cmap->x)<<16);
  uintptr_t updated_address_code = ((((uintptr_t)cmap->m)<<24) + sum) | ADDRESSING_BASEFLAG;
  uintptr_t address_code;
  if (sum & 0x01000000)
  {
    cmap->cpu_state = (R6502OP*)incremented_cpu_state;
    cmap->modifyoutofpage = 1;
    address_code = cmap->address_code;
  }
  else
  {
    cmap->cpu_state = ((R6502OP*)incremented_cpu_state)+1;
    cmap->modifyoutofpage = 0;
    address_code = updated_address_code;
  }
  cmap->address_code = updated_address_code;
  R6502_CYCLE_READ_RETURN(cmap, address_code)
}

R6502CPUMAP* r65c12loadabsxstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t sum = cmap->latch + (((uintptr_t)cmap->x)<<16);
  uintptr_t updated_address_code = ((((uintptr_t)cmap->m)<<24) + sum) | ADDRESSING_BASEFLAG;
  uintptr_t address_code;
  if (sum & 0x01000000)
  {
    cmap->cpu_state = (R6502OP*)incremented_cpu_state;
    address_code = cmap->address_code;
  }
  else
  {
    cmap->cpu_state = ((R6502OP*)incremented_cpu_state)+1;
    address_code = updated_address_code;
  }
  cmap->address_code = updated_address_code;
  R6502_CYCLE_READ_RETURN(cmap, address_code)
}

R6502CPUMAP* r65c12loadabsystep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t sum = cmap->latch + (((uintptr_t)cmap->y)<<16);
