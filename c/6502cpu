  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  cmap->ps &= (NOTNFLAG & NOTZFLAG & NOTCFLAG);
  cmap->a &= rotated;
  cmap->m = rotated & 0xFF;
  cmap->ps |= (cmap->a & NFLAG) | (cmap->a == 0 ? ZFLAG : 0) | ((rotated>>8) & CFLAG);
  R6502_CYCLE_WRITE_RETURN(cmap, cmap->address_code)
