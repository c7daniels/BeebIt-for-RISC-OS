#ifdef BEEBIT_NO_ASM
static inline void r6502startstretch1readspecial(R6502CPUMAP* cmap, uintptr_t address_code, R6502OP* cpu_state)
{
  cmap->stretched_address_code = address_code;
  cmap->stretched_cpu_state = cpu_state;
  cmap->cpu_state = cmap->sequences[R6502_READ_STRETCH_SEQUENCE]+1;
}

static inline void r6502startstretch2readspecial(R6502CPUMAP* cmap, uintptr_t address_code, R6502OP* cpu_state)
{
  cmap->stretched_address_code = address_code;
  cmap->stretched_cpu_state = cpu_state;
  cmap->cpu_state = cmap->sequences[R6502_READ_STRETCH_SEQUENCE];
}

static inline void r6502startstretch1discardspecial(R6502CPUMAP* cmap, uintptr_t address_code, R6502OP* cpu_state)
{
  cmap->stretched_address_code = address_code;
  cmap->stretched_cpu_state = cpu_state;
  cmap->cpu_state = cmap->sequences[R6502_DISCARD_STRETCH_SEQUENCE]+1;
}

static inline void r6502startstretch2discardspecial(R6502CPUMAP* cmap, uintptr_t address_code, R6502OP* cpu_state)
{
  cmap->stretched_address_code = address_code;
  cmap->stretched_cpu_state = cpu_state;
  cmap->cpu_state = cmap->sequences[R6502_DISCARD_STRETCH_SEQUENCE];
}

static inline void r6502startstretch1writespecial(R6502CPUMAP* cmap, uintptr_t address_code, R6502OP* cpu_state)
{
  cmap->stretched_address_code = address_code;
  cmap->stretched_cpu_state = cpu_state;
  cmap->cpu_state = cmap->sequences[R6502_WRITE_STRETCH_SEQUENCE]+1;
}

static inline void r6502startstretch2writespecial(R6502CPUMAP* cmap, uintptr_t address_code, R6502OP* cpu_state)
{
  cmap->stretched_address_code = address_code;
  cmap->stretched_cpu_state = cpu_state;
  cmap->cpu_state = cmap->sequences[R6502_WRITE_STRETCH_SEQUENCE];
}

R6502CPUMAP* r6502cyclereadmappedreturn(R6502CPUMAP* cmap, uint8_t* p)
{
  cmap->m = *p;
  R6502_CYCLE_RETURN(cmap)
}

R6502CPUMAP* r6502cyclereadspecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  cmap->readspecialaction(cmap, address_code);
  R6502_CYCLE_RETURN(cmap)
}

R6502CPUMAP* r6502cyclestretch1readspecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  r6502startstretch1readspecial(cmap, address_code, cmap->cpu_state);
  R6502_CYCLE_STRETCH_RETURN(cmap)
}

R6502CPUMAP* r6502cyclestretch2readspecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  r6502startstretch2readspecial(cmap, address_code, cmap->cpu_state);
  R6502_CYCLE_STRETCH_RETURN(cmap)
}

R6502CPUMAP* r6502cyclediscardmappedreturn(R6502CPUMAP* cmap, uint8_t* p)
{
  R6502_CYCLE_RETURN(cmap)
}

R6502CPUMAP* r6502cyclediscardspecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  cmap->latch = (((uintptr_t)cmap->m)<<16);
  cmap->readspecialaction(cmap, address_code);
  cmap->m = ((cmap->latch>>16) & 0xFF);
  R6502_CYCLE_RETURN(cmap)
}

R6502CPUMAP* r6502cyclestretch1discardspecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  r6502startstretch1discardspecial(cmap, address_code, cmap->cpu_state);
  R6502_CYCLE_STRETCH_RETURN(cmap)
}

R6502CPUMAP* r6502cyclestretch2discardspecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  r6502startstretch2discardspecial(cmap, address_code, cmap->cpu_state);
  R6502_CYCLE_STRETCH_RETURN(cmap)
}

R6502CPUMAP* r6502cyclewritemappedreturn(R6502CPUMAP* cmap, uint8_t* p)
{
  *p = cmap->m;
  R6502_CYCLE_RETURN(cmap)
}

R6502CPUMAP* r6502cyclewritespecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  cmap->writespecialaction(cmap, address_code, cmap->m);
  R6502_CYCLE_RETURN(cmap)
}

R6502CPUMAP* r6502cyclestretch1writespecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  r6502startstretch1writespecial(cmap, address_code, cmap->cpu_state);
  R6502_CYCLE_STRETCH_RETURN(cmap)
}

R6502CPUMAP* r6502cyclestretch2writespecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  r6502startstretch2writespecial(cmap, address_code, cmap->cpu_state);
  R6502_CYCLE_STRETCH_RETURN(cmap)
}

R6502CPUMAP* r6502cyclefetchmappedreturn(R6502CPUMAP* cmap, uint8_t* p)
{
  cmap->cpu_state = cmap->sequences[*p];

  if (cmap->transition1 == R6502_RESET_TRANSITION)
  {
    cmap->cpu_state = cmap->sequences[R6502_RESET_SEQUENCE];
  }
  else if (cmap->transition1 == R6502_YIELD_TRANSITION)
  {
    cmap->execute_cpu_state = cmap->cpu_state;
    cmap->cpu_state = cmap->sequences[R6502_YIELD_SEQUENCE];
  }
  R6502_CYCLE_RETURN(cmap)
}

R6502CPUMAP* r6502cyclefetchspecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  cmap->readspecialaction(cmap, address_code);
  return r6502cyclefetchmappedreturn(cmap, &cmap->m);
}

R6502CPUMAP* r6502cyclestretch1fetchspecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  r6502startstretch1readspecial(cmap, address_code, cmap->sequences[R6502_READ_STRETCH_SEQUENCE]+2);
  R6502_CYCLE_STRETCH_RETURN(cmap)
}

R6502CPUMAP* r6502cyclestretch2fetchspecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  r6502startstretch2readspecial(cmap, address_code, cmap->sequences[R6502_READ_STRETCH_SEQUENCE]+2);
  R6502_CYCLE_STRETCH_RETURN(cmap)
}

R6502CPUMAP* r6502cyclefetchafterreadspecial(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  return r6502cyclefetchmappedreturn(cmap, &cmap->m);
}

R6502CPUMAP* r6502cycleearlyinterruptfetchmappedreturn(R6502CPUMAP* cmap, uint8_t* p)
{
  uintptr_t interruptqueue = cmap->interruptqueue;

  cmap->cpu_state = cmap->sequences[*p];

  if (cmap->transition1 == R6502_RESET_TRANSITION)
  {
    cmap->cpu_state = cmap->sequences[R6502_RESET_SEQUENCE];
  }
  else if ((interruptqueue>>6) & interruptqueue & (R6502_NMI_IRQ_ENABLE<<12))
  {
    cmap->cpu_state = cmap->sequences[R6502_INTERRUPT_SEQUENCE];
  }
  else if (cmap->transition1 == R6502_YIELD_TRANSITION)
  {
    cmap->execute_cpu_state = cmap->cpu_state;
    cmap->cpu_state = cmap->sequences[R6502_YIELD_SEQUENCE];
  }
  R6502_CYCLE_RETURN(cmap)
}

R6502CPUMAP* r6502cycleearlyinterruptfetchspecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  cmap->readspecialaction(cmap, address_code);
  return r6502cycleearlyinterruptfetchmappedreturn(cmap, &cmap->m);
}

R6502CPUMAP* r6502cycleearlyinterruptstretch1fetchspecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  r6502startstretch1readspecial(cmap, address_code, cmap->sequences[R6502_READ_STRETCH_SEQUENCE]+3);
  R6502_CYCLE_STRETCH_RETURN(cmap)
}

R6502CPUMAP* r6502cycleearlyinterruptstretch2fetchspecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  r6502startstretch2readspecial(cmap, address_code, cmap->sequences[R6502_READ_STRETCH_SEQUENCE]+3);
  R6502_CYCLE_STRETCH_RETURN(cmap)
}

R6502CPUMAP* r6502cycleearlyinterruptfetchafterreadspecial(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  return r6502cycleearlyinterruptfetchmappedreturn(cmap, &cmap->m);
}

R6502CPUMAP* r6502cycleinterruptfetchmappedreturn(R6502CPUMAP* cmap, uint8_t* p)
{
  uintptr_t interruptqueue = cmap->interruptqueue;

  cmap->cpu_state = cmap->sequences[*p];

  if (cmap->transition1 == R6502_RESET_TRANSITION)
  {
    cmap->cpu_state = cmap->sequences[R6502_RESET_SEQUENCE];
  }
  else if ((interruptqueue>>6) & interruptqueue & (R6502_NMI_IRQ_ENABLE<<8))
  {
    cmap->cpu_state = cmap->sequences[R6502_INTERRUPT_SEQUENCE];
  }
  else if (cmap->transition1 == R6502_YIELD_TRANSITION)
  {
    cmap->execute_cpu_state = cmap->cpu_state;
    cmap->cpu_state = cmap->sequences[R6502_YIELD_SEQUENCE];
  }
  R6502_CYCLE_RETURN(cmap)
}

R6502CPUMAP* r6502cycleinterruptfetchspecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  cmap->readspecialaction(cmap, address_code);
  return r6502cycleinterruptfetchmappedreturn(cmap, &cmap->m);
}

R6502CPUMAP* r6502cycleinterruptstretch1fetchspecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  r6502startstretch1readspecial(cmap, address_code, cmap->sequences[R6502_READ_STRETCH_SEQUENCE]+4);
  R6502_CYCLE_STRETCH_RETURN(cmap)
}

R6502CPUMAP* r6502cycleinterruptstretch2fetchspecialreturn(R6502CPUMAP* cmap, uintptr_t address_code)
{
  r6502startstretch2readspecial(cmap, address_code, cmap->sequences[R6502_READ_STRETCH_SEQUENCE]+4);
  R6502_CYCLE_STRETCH_RETURN(cmap)
}

R6502CPUMAP* r6502cycleinterruptfetchafterreadspecial(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  return r6502cycleinterruptfetchmappedreturn(cmap, &cmap->m);
}

// IMPLIED STEPS
R6502CPUMAP* r6502operandstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  R6502_CYCLE_READ_RETURN(cmap, (cmap->sync_pc_code + 0x10000) & 0xFFFFFFFF)
}

R6502CPUMAP* r6502nopstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502clcstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->ps &= NOTCFLAG;
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502secstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->ps |= CFLAG;
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502clistep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->interruptqueue |= R6502_IRQ_ENABLE;
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502seistep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->interruptqueue &= ~R6502_IRQ_ENABLE;
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502cldstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->ps &= NOTDFLAG;
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502sedstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->ps |= DFLAG;
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502clvstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->ps &= NOTVFLAG;
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502taxstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->x = cmap->a;
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->x & NFLAG) | (cmap->x == 0 ? ZFLAG : 0);
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502taystep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->y = cmap->a;
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->y & NFLAG) | (cmap->y == 0 ? ZFLAG : 0);
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502txastep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->a = cmap->x;
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->a & NFLAG) | (cmap->a == 0 ? ZFLAG : 0);
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502txsstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->sp_code = (((uintptr_t)cmap->x)<<24);
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502tyastep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->a = cmap->y;
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->a & NFLAG) | (cmap->a == 0 ? ZFLAG : 0);
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502tsxstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->x = (cmap->sp_code>>24);
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->x & NFLAG) | (cmap->x == 0 ? ZFLAG : 0);
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502pulledastep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->a = cmap->m;
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->a & NFLAG) | (cmap->a == 0 ? ZFLAG : 0);
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502pulledpstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  if (cmap->m & IFLAG)
    cmap->interruptqueue &= ~R6502_IRQ_ENABLE;
  else
    cmap->interruptqueue |= R6502_IRQ_ENABLE;
  cmap->ps = cmap->m | UFLAG | BFLAG | IFLAG;
  R6502_CYCLE_IMPLIED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502hltstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  if (cmap->transition1 == R6502_RESET_TRANSITION)
  {
    cmap->cpu_state = cmap->sequences[R6502_RESET_SEQUENCE];
  }
  else if (cmap->transition1 == R6502_YIELD_TRANSITION)
  {
    cmap->execute_cpu_state = cmap->cpu_state;
    cmap->cpu_state = cmap->sequences[R6502_YIELD_SEQUENCE];
  }
  R6502_CYCLE_RETURN(cmap)
}

// LOAD STEPS
R6502CPUMAP* r6502beforepullstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  R6502_CYCLE_STACK_READ_RETURN(cmap, cmap->sp_code)
}

R6502CPUMAP* r6502topullstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  cmap->sp_code = (cmap->sp_code + 0x1000000) & 0xFFFFFFFF;
  R6502_CYCLE_STACK_READ_RETURN(cmap, cmap->sp_code)
}

R6502CPUMAP* r6502toimmediatestep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  cmap->last_pc_code = (cmap->sync_pc_code + 0x10000) & 0xFFFFFFFF;
  R6502_CYCLE_READ_RETURN(cmap, cmap->last_pc_code)
}

R6502CPUMAP* r6502breadcrumbloadzpgstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t z_code;
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  cmap->last_pc_code = (cmap->sync_pc_code + 0x10000) & 0xFFFFFFFF;
  z_code = (((uintptr_t)cmap->m)<<24);
  cmap->address_code = z_code;
  R6502_CYCLE_ZPG_READ_RETURN(cmap, z_code)
}

R6502CPUMAP* r6502loadzpgstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t z_code;
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  cmap->last_pc_code = (cmap->sync_pc_code + 0x10000) & 0xFFFFFFFF;
  z_code = (((uintptr_t)cmap->m)<<24);
  R6502_CYCLE_ZPG_READ_RETURN(cmap, z_code)
}

R6502CPUMAP* r6502breadcrumblatchabslostep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  cmap->last_pc_code = (cmap->sync_pc_code + 0x20000) & 0xFFFFFFFF;
  cmap->latch = (((uintptr_t)cmap->m)<<16);
  cmap->address_code = cmap->last_pc_code;
  R6502_CYCLE_READ_RETURN(cmap, cmap->last_pc_code)
}

R6502CPUMAP* r6502latchabslostep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  cmap->last_pc_code = (cmap->sync_pc_code + 0x20000) & 0xFFFFFFFF;
  cmap->latch = (((uintptr_t)cmap->m)<<16);
  R6502_CYCLE_READ_RETURN(cmap, cmap->last_pc_code)
}

R6502CPUMAP* r6502breadcrumbloadabsstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  cmap->address_code = (((uintptr_t)cmap->m)<<24) | cmap->latch | ADDRESSING_BASEFLAG;
  R6502_CYCLE_READ_RETURN(cmap, cmap->address_code)
}

R6502CPUMAP* r6502loadabsstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t address_code;
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  address_code = (((uintptr_t)cmap->m)<<24) | cmap->latch | ADDRESSING_BASEFLAG;
  R6502_CYCLE_READ_RETURN(cmap, address_code)
}

R6502CPUMAP* r6502breadcrumblatchzpgindlostep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t z_code;
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  z_code = (cmap->address_code+0x1000000) & 0xFF000000;
  cmap->latch = (((uintptr_t)cmap->m)<<16);
  cmap->address_code = z_code;
  R6502_CYCLE_ZPG_READ_RETURN(cmap, z_code)
}

R6502CPUMAP* r6502latchzpgindlostep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t z_code;
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  z_code = (cmap->address_code+0x1000000) & 0xFF000000;
  cmap->latch = (((uintptr_t)cmap->m)<<16);
  R6502_CYCLE_ZPG_READ_RETURN(cmap, z_code)
}

R6502CPUMAP* r6502breadcrumbloadzpgxstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t z_code;
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  z_code = (cmap->address_code+(((uintptr_t)cmap->x)<<24)) & 0xFF000000;
  cmap->address_code = z_code;
  R6502_CYCLE_ZPG_READ_RETURN(cmap, z_code)
}

R6502CPUMAP* r6502loadzpgxstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t z_code;
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  z_code = (cmap->address_code+(((uintptr_t)cmap->x)<<24)) & 0xFF000000;
  R6502_CYCLE_ZPG_READ_RETURN(cmap, z_code)
}

R6502CPUMAP* r6502loadzpgystep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t z_code;
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  z_code = (cmap->address_code+(((uintptr_t)cmap->y)<<24)) & 0xFF000000;
  R6502_CYCLE_ZPG_READ_RETURN(cmap, z_code)
}

R6502CPUMAP* r6502loadabsxstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t sum = cmap->latch + (((uintptr_t)cmap->x)<<16);
  uintptr_t address_code = ((((uintptr_t)cmap->m)<<24) + sum) | ADDRESSING_BASEFLAG;
  cmap->address_code = address_code;
  if (sum & 0x01000000)
  {
    cmap->cpu_state = (R6502OP*)incremented_cpu_state;
    address_code = (address_code - 0x01000000) & 0xFFFFFFFF;
  }
  else
    cmap->cpu_state = ((R6502OP*)incremented_cpu_state)+1;
  R6502_CYCLE_READ_RETURN(cmap, address_code)
}

R6502CPUMAP* r6502loadabsystep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t sum = cmap->latch + (((uintptr_t)cmap->y)<<16);
  uintptr_t address_code = ((((uintptr_t)cmap->m)<<24) + sum) | ADDRESSING_BASEFLAG;
  cmap->address_code = address_code;
  if (sum & 0x01000000)
  {
    cmap->cpu_state = (R6502OP*)incremented_cpu_state;
    address_code = (address_code - 0x01000000) & 0xFFFFFFFF;
  }
  else
    cmap->cpu_state = ((R6502OP*)incremented_cpu_state)+1;
  R6502_CYCLE_READ_RETURN(cmap, address_code)
}

R6502CPUMAP* r6502linearloadabsxstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t sum = cmap->latch + (((uintptr_t)cmap->x)<<16);
  uintptr_t address_code = ((((uintptr_t)cmap->m)<<24) + sum) | ADDRESSING_BASEFLAG;
  cmap->address_code = address_code;
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  if (sum & 0x01000000)
    address_code = (address_code - 0x01000000) & 0xFFFFFFFF;
  R6502_CYCLE_READ_RETURN(cmap, address_code)
}

R6502CPUMAP* r6502linearloadabsystep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  uintptr_t sum = cmap->latch + (((uintptr_t)cmap->y)<<16);
  uintptr_t address_code = ((((uintptr_t)cmap->m)<<24) + sum) | ADDRESSING_BASEFLAG;
  cmap->address_code = address_code;
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  if (sum & 0x01000000)
    address_code = (address_code - 0x01000000) & 0xFFFFFFFF;
  R6502_CYCLE_READ_RETURN(cmap, address_code)
}

R6502CPUMAP* r6502fixindexedstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  R6502_CYCLE_READ_RETURN(cmap, cmap->address_code)
}

R6502CPUMAP* r6502bitstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  int aandm = cmap->a & cmap->m;
  cmap->ps &= (NOTNFLAG & NOTVFLAG & NOTZFLAG);
  cmap->ps |= (cmap->m & (NFLAG | VFLAG)) | (aandm == 0 ? ZFLAG : 0);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502orastep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->a |= cmap->m;
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->a & NFLAG) | (cmap->a == 0 ? ZFLAG : 0);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502andstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->a &= cmap->m;
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->a & NFLAG) | (cmap->a == 0 ? ZFLAG : 0);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502eorstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->a ^= cmap->m;
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->a & NFLAG) | (cmap->a == 0 ? ZFLAG : 0);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502adcstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  unsigned int m = cmap->m;
  unsigned int a = cmap->a;
  uintptr_t sum;
  m = ((m<<4)&0xF00) | (m&0xF);
  a = ((a<<4)&0xF00) | (a&0xF);
  sum = a + 0x0F0 + m + (cmap->ps & CFLAG);
  cmap->ps &= (NOTNFLAG & NOTVFLAG & NOTZFLAG & NOTCFLAG);
  cmap->ps |= ((sum & 0xF0F) == 0 ? ZFLAG : 0);
  if (cmap->ps & DFLAG)
    sum += 6;
  cmap->ps |= ((sum>>4) & NFLAG) | ((((sum ^ a) & (sum ^ m))>>5) & VFLAG);
  if (cmap->ps & DFLAG)
  {
    if (sum & 0x80)
      sum -= 6;
    if (sum >= 0xA00)
      sum += 0x600;
  }
  if (sum >= 0x1000)
    cmap->ps |= CFLAG;
  cmap->a = ((sum>>4)&0xF0) | (sum&0xF);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502ldastep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->a = cmap->m;
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->a & NFLAG) | (cmap->a == 0 ? ZFLAG : 0);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502ldxstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->x = cmap->m;
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->x & NFLAG) | (cmap->x == 0 ? ZFLAG : 0);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502ldystep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->y = cmap->m;
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->y & NFLAG) | (cmap->y == 0 ? ZFLAG : 0);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502cmpstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  unsigned int d = 0x100 + cmap->a - cmap->m;
  cmap->ps &= (NOTNFLAG & NOTZFLAG & NOTCFLAG);
  cmap->ps |= (d & NFLAG) | ((d & 0xFF) == 0 ? ZFLAG : 0) | ((d>>8) & CFLAG);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502cpxstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  unsigned int d = 0x100 + cmap->x - cmap->m;
  cmap->ps &= (NOTNFLAG & NOTZFLAG & NOTCFLAG);
  cmap->ps |= (d & NFLAG) | ((d & 0xFF) == 0 ? ZFLAG : 0) | ((d>>8) & CFLAG);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502cpystep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  unsigned int d = 0x100 + cmap->y - cmap->m;
  cmap->ps &= (NOTNFLAG & NOTZFLAG & NOTCFLAG);
  cmap->ps |= (d & NFLAG) | ((d & 0xFF) == 0 ? ZFLAG : 0) | ((d>>8) & CFLAG);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502sbcstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  unsigned int im = 0xFF ^ cmap->m;
  unsigned int a = cmap->a;
  uintptr_t sum;
  im = ((im<<4)&0xF00) | (im&0xF);
  a = ((a<<4)&0xF00) | (a&0xF);
  sum = a + 0x0F0 + im + (cmap->ps & CFLAG);
  cmap->ps &= (NOTNFLAG & NOTVFLAG & NOTZFLAG & NOTCFLAG);
  cmap->ps |= ((sum & 0xF0F) == 0 ? ZFLAG : 0) | ((sum>>12) & CFLAG);
  cmap->ps |= ((sum>>4) & NFLAG) | ((((sum ^ a) & (sum ^ im))>>5) & VFLAG);
  if (cmap->ps & DFLAG)
  {
    if (sum & 0x80)
      sum -= 6;
    if (!(cmap->ps & CFLAG))
      sum -= 0x600;
  }
  cmap->a = ((sum>>4)&0xF0) | (sum&0xF);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502skipstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502laxstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->x = cmap->a = cmap->m;
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->a & NFLAG) | (cmap->a == 0 ? ZFLAG : 0);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502lasstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  unsigned int anded = (cmap->sp_code>>24) & cmap->m;
  cmap->x = cmap->a = anded;
  cmap->sp_code = (anded<<24);
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->x & NFLAG) | (cmap->x == 0 ? ZFLAG : 0);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502ancstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->a &= cmap->m;
  cmap->ps &= (NOTNFLAG & NOTZFLAG & NOTCFLAG);
  cmap->ps |= (cmap->a & NFLAG) | (cmap->a == 0 ? ZFLAG : 0) | ((cmap->a>>7) & CFLAG);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502alrstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  unsigned int anded = cmap->a & cmap->m;
  cmap->ps &= (NOTNFLAG & NOTZFLAG & NOTCFLAG);
  cmap->a = ((anded>>1) & 0x7F);
  cmap->ps |= (cmap->a == 0 ? ZFLAG : 0) | (anded & CFLAG);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502arrstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  unsigned int anded = cmap->a & cmap->m;
  uintptr_t rotated = ((cmap->ps & CFLAG)<<7) | (anded>>1);
  cmap->ps &= (NOTNFLAG & NOTVFLAG & NOTZFLAG & NOTCFLAG);
  cmap->ps |= (rotated & NFLAG) | ((anded^rotated) & VFLAG) | ((rotated & 0xFF) == 0 ? ZFLAG : 0);
  if (cmap->ps & DFLAG)
  {
    if (anded >= 0x50)
    {
      cmap->ps |= CFLAG;
      rotated += 0x60;
    }
    if ((anded & 0x0F) >= 0x05)
      rotated = (rotated & 0xF0) | ((rotated + 0x06) & 0x0F);
  }
  else
  {
    cmap->ps |= ((anded>>7) & CFLAG);
  }
  cmap->a = (rotated & 0xFF);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502xaastep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->a = cmap->x & cmap->m;
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->a & NFLAG) | (cmap->a == 0 ? ZFLAG : 0);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502oalstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->x = cmap->a = (cmap->a | 0xEE) & cmap->m;
  cmap->ps &= (NOTNFLAG & NOTZFLAG);
  cmap->ps |= (cmap->a & NFLAG) | (cmap->a == 0 ? ZFLAG : 0);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}

R6502CPUMAP* r6502sbxstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  unsigned int d = 0x100 + (cmap->a & cmap->x) - cmap->m;
  cmap->x = (d & 0xFF);
  cmap->ps &= (NOTNFLAG & NOTZFLAG & NOTCFLAG);
  cmap->ps |= (d & NFLAG) | (cmap->x == 0 ? ZFLAG : 0) | ((d>>8) & CFLAG);
  R6502_CYCLE_ADDRESSED_INTERRUPT_CHECK_FETCH_RETURN(cmap)
}
