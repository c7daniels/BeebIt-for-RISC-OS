  R6502_CYCLE_STACK_WRITE_RETURN(cmap, cmap->sp_code)
}

R6502CPUMAP* r6502pushedtojsrpushpclstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  cmap->sp_code = (cmap->sp_code - 0x1000000) & 0xFFFFFFFF;
  cmap->m = ((cmap->last_pc_code>>16) & 0xFF);
  R6502_CYCLE_STACK_WRITE_RETURN(cmap, cmap->sp_code)
}

R6502CPUMAP* r6502pushedtoloadpchstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  cmap->sp_code = (cmap->sp_code - 0x1000000) & 0xFFFFFFFF;
  R6502_CYCLE_READ_RETURN(cmap, cmap->last_pc_code)
}

R6502CPUMAP* r6502pulledptopullstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  cmap->sp_code = (cmap->sp_code + 0x1000000) & 0xFFFFFFFF;
  if (cmap->m & IFLAG)
    cmap->interruptqueue &= ~R6502_IRQ_ENABLE;
  else
    cmap->interruptqueue |= R6502_IRQ_ENABLE;
  cmap->ps = cmap->m | UFLAG | BFLAG | IFLAG;
  R6502_CYCLE_STACK_READ_RETURN(cmap, cmap->sp_code)
}

R6502CPUMAP* r6502pulledpclstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  cmap->sp_code = (cmap->sp_code + 0x1000000) & 0xFFFFFFFF;
  cmap->latch = (((uintptr_t)cmap->m)<<16);
  R6502_CYCLE_STACK_READ_RETURN(cmap, cmap->sp_code)
}

R6502CPUMAP* r6502loadpchtoincrementstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  cmap->last_pc_code = (((uintptr_t)cmap->m)<<24) | cmap->latch | ADDRESSING_BASEFLAG;
  R6502_CYCLE_READ_RETURN(cmap, cmap->last_pc_code)
}

R6502CPUMAP* r6502midstretchstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = (R6502OP*)incremented_cpu_state;
  R6502_CYCLE_STRETCH_RETURN(cmap)
}

R6502CPUMAP* r6502stretchedreadstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = cmap->stretched_cpu_state;
  cmap->readspecialaction(cmap, cmap->stretched_address_code);
  R6502_CYCLE_RETURN(cmap)
}

R6502CPUMAP* r6502stretcheddiscardstep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = cmap->stretched_cpu_state;
  cmap->latch = (((uintptr_t)cmap->m)<<16);
  cmap->readspecialaction(cmap, cmap->stretched_address_code);
  cmap->m = ((cmap->latch>>16) & 0xFF);
  R6502_CYCLE_RETURN(cmap)
}

R6502CPUMAP* r6502stretchedwritestep(R6502CPUMAP* cmap, uintptr_t incremented_cpu_state)
{
  cmap->cpu_state = cmap->stretched_cpu_state;
  cmap->writespecialaction(cmap, cmap->stretched_address_code, cmap->m);
  R6502_CYCLE_RETURN(cmap)
}

R6502CPUMAP* r6502startyield(R6502CPUMAP* cmap)
{
  cmap->transition2 = R6502_YIELD_TRANSITION;

  if (cmap->transition1 == R6502_REGULAR_TRANSITION)
  {
    cmap->transition1 = R6502_YIELD_TRANSITION;
    R6502_CHECK_FOR_BREAK(cmap)
  }
  return cmap;
}
#endif

static inline int r6502setsequences(R6502CPUMAP* cmap, int index, int count, const R6502OP* const *sequences)
{
  int lreturn;
  int i;
  const R6502OP* s;
  R6502OP* entry = &cmap->steps[index<<R6502_LOG2_MAX_SEQUENCE_LENGTH];

  lreturn = TRUE;

  for ( ; count > 0; count--)
  {
    s = *sequences++;

    i=0;
    if (s)
    {
      for (; s[i]; i++, entry++)
      {
        if (i == (1<<R6502_LOG2_MAX_SEQUENCE_LENGTH))
        {
          lreturn = FALSE;
          break;
        }

        *entry = s[i];
      }
    }
    for (; i<(1<<R6502_LOG2_MAX_SEQUENCE_LENGTH); i++, entry++)
    {
      *entry = NULL;
    }
  }

  return lreturn;
}

int r6502reset(R6502CPUMAP* cmap, const R6502OP* const* mainsequences, const R6502OP* const* extrasequences)
{
  R6502OP* entry = cmap->steps;
  R6502OP* s;
  int i;

  /*it is assumed that shift fields for AArch32 byte/word aliasing
   have been initialised to zero at load time or by calloc()*/

  cmap->oldreset = FALSE;
  cmap->lbreak = FALSE;

  cmap->sync_pc_code = ADDRESSING_BASEFLAG;
  cmap->sp_code = 0xFF000000;
  cmap->ps = UFLAG | BFLAG | IFLAG;

  if (!r6502setsequences(cmap, 0, R6502_MAIN_SEQUENCE_COUNT, mainsequences))
    return TRUE;

  if (!r6502setsequences(cmap, R6502_MAIN_SEQUENCE_COUNT, R6502_EXTRA_SEQUENCE_COUNT, extrasequences))
    return TRUE;

  for (i=0; i<R6502_SEQUENCE_COUNT; i++)
  {
    cmap->sequences[i] = entry;
    entry += (1<<R6502_LOG2_MAX_SEQUENCE_LENGTH);
  }

  s = cmap->sequences[R6502_OPCODE_07_SEQUENCE];
  if (*s)
  {
    cmap->sequences[R6502_OPCODE_07_SEQUENCE] = cmap->sequences[0x07];
    cmap->sequences[0x07] = s;
  }

  cmap->execute_cpu_state = cmap->sequences[R6502_RESET_SEQUENCE];
  cmap->address_code = (cmap->address_code & 0xFFFF0000) | ADDRESSING_BASEFLAG;

  return FALSE;
}

static R6502CPUMAP* r6502readmemoryread(R6502CPUMAP* cmap, uint8_t* p)
{
  cmap->m = *p;
  return cmap;
}

void r6502readmemory(R6502CPUMAP* cmap, char* buffer, uintptr_t naddress, int ncount)
{
  unsigned char savedm = cmap->m;
  uintptr_t address_code = ((naddress<<16) & 0xFFFF0000) | ADDRESSING_BASEFLAG;
  R6502READSPECIALOP rds = cmap->readspecialaction;

  for ( ; ncount > 0; ncount--, buffer++)
  {
    R6502_ADDRESS_FOR_READ(cmap, address_code, rds, rds, rds, r6502readmemoryread)
    *buffer = cmap->m;

    address_code = (address_code + 0x10000) & 0xFFFFFFFF;
  }

  cmap->m = savedm;
}

void r6502beforesnapshot(R6502CPUMAP* cmap)
{
  if (cmap->interruptqueue & R6502_IRQ_ENABLE)
    cmap->ps &= NOTIFLAG;
}

void r6502aftersnapshot(R6502CPUMAP* cmap)
{
  uintptr_t address_code = ((cmap->sync_pc_code<<16) & 0xFFFF0000) | ADDRESSING_BASEFLAG;
  /*a refetch from an I/O address can have side-effects and/or yield a different opcode*/
  R6502READSPECIALOP rds = cmap->readspecialaction;

  R6502_ADDRESS_FOR_READ(cmap, address_code, rds, rds, rds, r6502readmemoryread)
  cmap->execute_cpu_state = cmap->sequences[cmap->m];

  if (cmap->ps & IFLAG)
    cmap->interruptqueue &= ~R6502_IRQ_ENABLE;
  else
  {
    cmap->interruptqueue |= R6502_IRQ_ENABLE;
    cmap->ps |= IFLAG;
  }
}
