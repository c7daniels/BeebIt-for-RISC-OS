HOSTMAP* videoularate0oshift(HOSTMAP* hmap, uintptr_t value)
{
  hmap->B_crtc.step_state -= VIDEOSTEPIDLEFLAG;
  hmap->A_videoula.shiftregister = (hmap->A_videoula.shiftregister<<1)|0x01;
  return teletextonesliceloadeven(hmap, value);
}

HOSTMAP* videoularate0oshiftR(HOSTMAP* hmap, uintptr_t value)
{
  hmap->B_crtc.step_state -= VIDEOSTEPIDLEFLAG;
  hmap->A_videoula.shiftregister = (hmap->A_videoula.shiftregister<<1)|0x01;
  return teletextonesliceloadevenR(hmap, value);
}

