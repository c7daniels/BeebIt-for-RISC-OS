static inline HOSTMAP* teletextloadoneslice(HOSTMAP* hmap, uintptr_t queue)
{
  hmap->B_teletext.queue = queue;
  return hmap->teletextops[queue>>23](hmap);
}

static inline HOSTMAP* teletextloadrounding(HOSTMAP* hmap, uintptr_t queue)
{
  hmap->B_teletext.queue = queue;
  hmap->teletextops[queue>>23](hmap);

  if (hmap->A_crtc.c9low & 1)
  {
    hmap->A_teletext.roundingdirection1 = 1;
    hmap->B_teletext.lue1 += hmap->B_teletext.lowoffset;
  }
  else
    hmap->A_teletext.roundingdirection1 = 0;
  return hmap;
}

static inline HOSTMAP* teletextshiftcommon(HOSTMAP* hmap)
{
  uintptr_t colours;

  hmap->B_teletext.lue2 = hmap->B_teletext.lue1;
  hmap->B_teletext.lue1 += hmap->B_teletext.rightoffset;

  colours = hmap->B_teletext.colours & 0xFF;
  hmap->B_teletext.colours = (colours<<8) | colours;
  hmap->B_teletext.fg2fill = hmap->B_teletext.fg1fill;
  hmap->B_teletext.bg2fill = hmap->B_teletext.bg1fill;
  return hmap;
}

static inline HOSTMAP* teletextshiftoneslice(HOSTMAP* hmap)
{
  return teletextshiftcommon(hmap);
}

static inline HOSTMAP* teletextshiftrounding(HOSTMAP* hmap)
{
  hmap->A_teletext.roundingdirection2 = hmap->A_teletext.roundingdirection1;
  return teletextshiftcommon(hmap);
}

static inline HOSTMAP* teletextonesliceloadeven(HOSTMAP* hmap, uintptr_t value)
{
  uintptr_t queue = hmap->B_teletext.queue;
  if (hmap->peripheral_code&VIDEODISPLAYDISPLAY)
    value |= 0x80;
  else
    value &= ~0x80;
  queue = ((queue | value)<<8) & 0xFFFFFFFF;
  return teletextloadoneslice(hmap, queue);
}

static inline HOSTMAP* teletextonesliceloadevenR(HOSTMAP* hmap, uintptr_t value)
{
  return teletextloadoneslice(hmap, (hmap->B_teletext.queue<<8) & 0xFFFFFFFF);
}

static inline HOSTMAP* teletextonesliceloadodd(HOSTMAP* hmap, uintptr_t value)
{
  if (hmap->B_teletext.queue & 0x80800000U)
    return teletextshiftoneslice(hmap);
  return hmap;
}

static inline HOSTMAP* teletextonesliceloadoddR(HOSTMAP* hmap, uintptr_t value)
{
  uintptr_t queue = hmap->B_teletext.queue;
  hmap->B_teletext.queue = queue & ~(0xFF<<8);
  if (queue & 0x80800000U)
    return teletextshiftoneslice(hmap);
  return hmap;
}

static inline HOSTMAP* teletextloadeven(HOSTMAP* hmap, uintptr_t value)
{
  uintptr_t queue = hmap->B_teletext.queue;
  if (hmap->peripheral_code&VIDEODISPLAYDISPLAY)
    value |= 0x80;
  else
    value &= ~0x80;
  queue = ((queue | value)<<8) & 0xFFFFFFFF;
  return teletextloadrounding(hmap, queue);
}

static inline HOSTMAP* teletextloadevenR(HOSTMAP* hmap, uintptr_t value)
{
  return teletextloadrounding(hmap, (hmap->B_teletext.queue<<8) & 0xFFFFFFFF);
}

static inline HOSTMAP* teletextloadodd(HOSTMAP* hmap, uintptr_t value)
{
  if (hmap->B_teletext.queue & 0x80800000U)
    return teletextshiftrounding(hmap);
  return hmap;
}

static inline HOSTMAP* teletextloadoddR(HOSTMAP* hmap, uintptr_t value)
{
  uintptr_t queue = hmap->B_teletext.queue;
  hmap->B_teletext.queue = queue & ~(0xFF<<8);
  if (queue & 0x80800000U)
    return teletextshiftrounding(hmap);
  return hmap;
}

static inline HOSTMAP* teletextprogressiveloadeven(HOSTMAP* hmap, uintptr_t value)
{
  uintptr_t queue = hmap->B_teletext.queue;
  value &= 0x7F;
  if (hmap->peripheral_code&VIDEODISPLAYDISPLAY)
    value |= 0x80;
  queue = ((queue | value)<<8) & 0xFFFFFFFF;
  return teletextloadrounding(hmap, queue);
}

HOSTMAP* videoulaoloadfasteven(HOSTMAP* hmap, uintptr_t value)
{
  hmap->A_videoula.shiftregister = (uint32_t)value;
  return teletextonesliceloadeven(hmap, value);
}

HOSTMAP* videoulaoloadfastevenR(HOSTMAP* hmap, uintptr_t value)
{
  hmap->A_videoula.shiftregister = (uint32_t)value;
  return teletextonesliceloadevenR(hmap, value);
}

HOSTMAP* videoulaoloadfastodd(HOSTMAP* hmap, uintptr_t value)
{
  hmap->A_videoula.shiftregister = (uint32_t)value;
  return teletextonesliceloadodd(hmap, value);
}

HOSTMAP* videoulaoloadfastoddR(HOSTMAP* hmap, uintptr_t value)
{
  hmap->A_videoula.shiftregister = (uint32_t)value;
  return teletextonesliceloadoddR(hmap, value);
}

HOSTMAP* videoulaoloadslowodd(HOSTMAP* hmap, uintptr_t value)
{
  hmap->B_crtc.step_state += VIDEOSTEPIDLEFLAG;
  hmap->A_videoula.shiftregister = (uint32_t)value;
  return teletextonesliceloadodd(hmap, value);
}

HOSTMAP* videoulaoloadslowoddR(HOSTMAP* hmap, uintptr_t value)
{
  hmap->B_crtc.step_state += VIDEOSTEPIDLEFLAG;
  hmap->A_videoula.shiftregister = (uint32_t)value;
  return teletextonesliceloadoddR(hmap, value);
}

